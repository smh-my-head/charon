# vi: set ft=gitconfig:
[diff "sldprt_diff"]
	command = ./sldworks-git-tools/sldprt_diff.sh
[diff "sldasm_diff"]
	command = ./sldworks-git-tools/sldasm_diff.sh
[merge "sldprt_merge"]
	driver  = ./sldworks-git-tools/sldprt_mergetool.sh %O %A %B
[merge "sldasm_merge"]
	driver  = ./sldworks-git-tools/sldasm_mergetool.sh %O %A %B
